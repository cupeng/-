.container{
    display:flex;
    flex-direction:column;
    align-items:center;
}
.user-avatar{
    width:200rpx;
    height:200rpx;
    margin-top:160rpx;
    border-radius:50%;
    margin-bottom:100rpx;
}

.user-name{
    font-size:22rpx;
    line-height:80rpx;
    font-weight:bold;
}

.moto{
    margin-top:200rpx;
    border:1rpx solid #405f80;
    width:200rpx;
    height:80rpx;
    border-radius:5px;
    text-align:center;
    color:#405f80;
}
